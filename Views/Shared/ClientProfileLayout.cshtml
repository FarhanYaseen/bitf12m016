<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
     <link rel="shortcut icon" href="~/Content/Images/logo.png" />
  

</head>
<body>
    <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                        <i class="icon-reorder shaded"></i></a><a class="brand" href="/Client/NewsFeed" style="font-weight: bold; color: darkgreen; font-family: 'Old English Text MT'; font-size: xx-large;">Social Pedia </a>
                    <div class="nav-collapse collapse navbar-inverse-collapse">
                        <ul class="nav nav-icons">
                            <li class="#"><a href="#"><i class="icon-envelope"></i></a></li>
                            <li><a href="#"><i class="icon-eye-open"></i></a></li>
                            <li><a href="#"><i class="icon-bar-chart"></i></a></li>
                        </ul>
                        <form class="navbar-search pull-left input-append" action="#">
                        <input type="text" class="span3">
                        <button class="btn" type="button">
                            <i class="icon-search"></i>
                        </button>
                        </form>
                        <ul class="nav pull-right">
                            <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Dropdown
                                <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Item No. 1</a></li>
                                    <li><a href="#">Don't Click</a></li>
                                    <li class="divider"></li>
                                    <li class="nav-header">Example Header</li>
                                    <li><a href="#">A Separated link</a></li>
                                </ul>
                            </li>
                            <li><a href="#">Support </a></li>
                            <li class="nav-user dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/AdminSide/images/user.png" class="nav-avatar" />
                                <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="/Client/Profile">Your Profile</a></li>
                                    <li><a href="#">Edit Profile</a></li>
                                    <li><a href="#">Account Settings</a></li>
                                    <li class="divider"></li>
                                    <li><a href="#">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- /.nav-collapse -->
                </div>
            </div>
            <!-- /navbar-inner -->
        </div>





    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link type="text/css" href="~/AdminSide/bootstrap/css/bootstrap.min.css" rel="stylesheet">
<link type="text/css" href="~/AdminSide/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">
<link type="text/css" href="~/AdminSide/css/theme.css" rel="stylesheet">
<link type="text/css" href="~/AdminSide/images/icons/css/font-awesome.css" rel="stylesheet">
<link type="text/css" href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600'
    rel='stylesheet'>

   
    <!-- /navbar -->
    <div class="wrapper">
        <div class="container">
            <div class="row">
                <div class="span3">
                    <div class="sidebar">
                        <ul class="widget widget-menu unstyled">
                            <li class="active"><a href="/Client/Profile"><i class="menu-icon icon-dashboard"></i>Profile
                            </a></li>
                            <li><a href="/Client/NewsFeed"><i class="menu-icon icon-bullhorn"></i>News Feed </a>
                            </li>
                            <li><a href="#"><i class="menu-icon icon-inbox"></i>Inbox <b class="label green pull-right">11</b> </a></li>
                            <li><a href="#"><i class="menu-icon icon-tasks"></i>Tasks <b class="label orange pull-right">19</b> </a></li>
                        </ul>
                        <!--/.widget-nav-->


                        <ul class="widget widget-menu unstyled">
                            <li><a href="#"><i class="menu-icon icon-bold"></i>Add Post(s) </a></li>
                            <li><a href="#"><i class="menu-icon icon-book"></i>Delete Post(s) </a></li>
                            <li><a href="#"><i class="menu-icon icon-paste"></i>Add Friends(s) </a></li>
                            <li><a href="#"><i class="menu-icon icon-table"></i>Delete Friends(s)  </a></li>
                            <li><a href="#"><i class="menu-icon icon-bar-chart"></i>Send Message(s) </a></li>
                        </ul>
                        <!--/.widget-nav-->
                        <ul class="widget widget-menu unstyled">
                            <li><a class="collapsed" data-toggle="collapse" href="#togglePages"><i class="menu-icon icon-cog"></i><i class="icon-chevron-down pull-right"></i><i class="icon-chevron-up pull-right"></i>More Pages </a>
                                <ul id="togglePages" class="collapse unstyled">
                                    <li><a href="#"><i class="icon-inbox"></i>Login </a></li>
                                    <li><a href="/Client/Profile"><i class="icon-inbox"></i>Profile </a></li>
                                    <li><a href="#"><i class="icon-inbox"></i>All Users </a></li>
                                </ul>
                            </li>
                            <li><a href="#"><i class="menu-icon icon-signout"></i>Logout </a></li>
                        </ul>
                    </div>
                    <!--/.sidebar-->
                </div>
                <!--/.span3-->
                <div class="span8">
                  
                        <div>
                            @RenderBody()
    @Scripts.Render("~/bundles/jquery")

    @RenderSection("scripts", required: false)
                        </div>

                        
                  
                    <!--/#btn-controls-->

                    <!--/.module-->
                </div>
                <!--/.content-->
            </div>
            <!--/.span9-->
        </div>
    </div>
    <!--/.container-->
    <!--/.wrapper-->

    <script src="~/AdminSide/scripts/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="~/AdminSide/scripts/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
    <script src="~/AdminSide/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/AdminSide/scripts/flot/jquery.flot.js" type="text/javascript"></script>
    <script src="~/AdminSide/scripts/flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="~/AdminSide/scripts/datatables/jquery.dataTables.js" type="text/javascript"></script>
    <script src="~/AdminSide/scripts/common.js" type="text/javascript"></script>

    


       <div class="footer">
            <div class="container">
                 <p style="margin-left:30%">
                        &copy; 2015-3000 <i  style="font-family: 'Old English Text MT'; font-size: xx-large">Social Pedia</i> , Inc. All Rights Reserved.
       
                    </p>  
            </div>
        </div>
</body>
</html>