@model List<Project_EAD.Models.User>
@{
    Layout = null;
}




<link href="~/Content/jquery.fancybox.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.10.1.min1.js"></script>
<script src="~/Scripts/jquery.fancybox.js"></script>
<script src="~/Scripts/jquery-1.10.1.min1.js"></script>
<script>
    $(document).ready(function () {
        /*
         *  Simple image gallery. Uses default settings
         */

        $('.fancybox').click(function () {
            $('.fancybox').fancybox({
                wrapCSS: 'fancybox-custom',
                closeClick: true,

                openEffect: 'none',

                helpers: {
                    title: {
                        type: 'inside'
                    },
                    overlay: {
                        css: {
                            'background': 'rgba(238,238,238,0.85)'
                        }
                    }
                }
            });
        });

    });

</script>
<div class="content">
    <div class="module">
        <div class="module-head">
            <h2>Search Results</h2>
        </div>


        <div class="module-body">


            <div class="row-fluid">

                @if (Model.Count == 0)
                {   
                    <h1>No Data Find</h1>
                }
                @foreach (var j in Model)
                { 
                    <div style="margin-left:10%" class="span9">
                        <div class="media user">
                            <a class="media-avatar pull-left" href="/Home/SearchResult">
                                <img src="@j.ImagePath">
                            </a>
                            <div class="media-body">
                                <h3 class="media-title">
                                    @j.Name</h3>
                                <p>
                                    <small class="muted">Pakistan</small>
                                </p>


                                <div class="media-option btn-group shaded-icon">
                                </div>
                            </div>
                        </div>

                    </div>
                }

            </div>
            

        </div>
    </div>
</div>
